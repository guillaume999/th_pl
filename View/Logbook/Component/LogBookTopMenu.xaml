<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="th_pl.View.Logbook.Component.LogBookTopMenu">
    <ContentView.Content>
        <Grid ColumnSpacing="2">
            <Grid.RowDefinitions>
                <RowDefinition />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="10*" />
                <ColumnDefinition Width="90*"/>
            </Grid.ColumnDefinitions>

            <Image
                Grid.Row="0"
                Grid.Column="0"
                Source="pref_param.png"
                Aspect="AspectFit"
                HeightRequest="78"
                BackgroundColor="#416CB5">
            </Image>

            <Image
                Grid.Row="0"
                Grid.Column="1"
                Source="logo_mini_sans_titre.png"
                Aspect="AspectFit"
                HeightRequest="78"
                BackgroundColor="#416CB5">
            </Image>


        </Grid>

    </ContentView.Content>
</ContentView>
